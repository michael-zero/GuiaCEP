{
  "expo": {
    "name": "guia-cep",
    "slug": "guia-cep",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CEDEE5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.infinitecode.guiacep",
      "config":{
        "googleMaps": {
          "apiKey": "COLE SUA CHAVE AQUI"
        }
      },
      "permissions": ["LOCATION","CAMERA", "READ_CONTACTS","READ_PHONE_STATE" ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CEDEE5"
      },
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
